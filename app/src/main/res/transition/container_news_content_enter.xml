<?xml version="1.0" encoding="utf-8"?>
<transitionSet xmlns:android="http://schemas.android.com/apk/res/android">

    <fade
        android:duration="100"
        android:fadingMode="fade_in">
        <targets>
            <target android:targetId="@+id/container_news_content_root" />
        </targets>
    </fade>

    <slide
        android:duration="@dimen/anim_default_duration"
        android:interpolator="@android:interpolator/decelerate_cubic"
        android:slideEdge="top">
        <targets>
            <target android:targetId="@+id/container_news_content_toolbar" />
        </targets>
    </slide>

    <slide
        android:duration="@dimen/anim_default_duration"
        android:interpolator="@android:interpolator/decelerate_cubic"
        android:slideEdge="top"
        android:startDelay="@dimen/anim_default_delay2">
        <targets>
            <target android:targetId="@+id/container_news_content_tv" />
        </targets>
    </slide>


